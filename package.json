{
  "name": "kraxler",
  "version": "0.1.0",
  "description": "Extract invoices from Gmail with AI-powered classification for Austrian tax",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "kraxler": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "scan": "node dist/index.js scan",
    "extract": "node dist/index.js extract",
    "crawl": "node dist/index.js crawl",
    "review": "node dist/index.js review",
    "run": "node dist/index.js run",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "invoice",
    "gmail",
    "automation",
    "pdf",
    "tax",
    "austria"
  ],
  "author": "Manuel Maly",
  "license": "MIT",
  "dependencies": {
    "@mariozechner/pi-agent-core": "^0.30.2",
    "@mariozechner/pi-ai": "^0.30.2",
    "@mariozechner/pi-coding-agent": "^0.30.2",
    "better-sqlite3": "^12.5.0",
    "commander": "^12.1.0",
    "env-paths": "^3.0.0",
    "highlight.js": "^11.11.1",
    "puppeteer-core": "^24.34.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
